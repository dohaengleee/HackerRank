    "import sys\n",
    "\n",
    "def repeatedString(s, n):\n",
    "    \n",
    "    results = (s.count(\"a\") * (n // len(s)) + s[:n % len(s)].count(\"a\"))\n",
    "    \n",
    "    return results\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "    s = input().strip()\n",
    "    n = int(input().strip())\n",
    "    result = repeatedString(s, n)\n",
    "    print(result)\n"
   ]
  }
